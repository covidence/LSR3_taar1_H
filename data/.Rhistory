responder_response<-function(value, n, mean, sd){
prop_less_value<-pnorm(value, mean, sd, lower.tail = TRUE)
n_participants<-n*prop_less_value
n_participants<-as.integer(round2(n_participants,0))
print(n_participants)
}
responder_response(-0.5*(102-30), 142, -16.9, 19.07)
round2 = function(x, digits) {
posneg = sign(x)
z = abs(x)*10^digits
z = z + 0.5 + sqrt(.Machine$double.eps)
z = trunc(z)
z = z/10^digits
z*posneg
}
