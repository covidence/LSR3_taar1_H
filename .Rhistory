width=0, position=position_dodge(width = 0.5))+
geom_vline(xintercept=1, linetype='dashed')+
scale_size_continuous(guide=FALSE)+
facet_grid(factor(MedDRA_group, levels=lever_order_meddra)~., scales = "free", space = "free")+
